{
  "name": "react-simple-step-wizard",
  "version": "0.1.0",
  "description": "React simple step wizard",
  "keywords": [
    "multistep",
    "react",
    "step",
    "wizard"
  ],
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "jsnext:main": "lib/index.es.js",
  "scripts": {
    "build": "rollup -c",
    "cz": "git-cz",
    "deploy": "gh-pages -d example/build",
    "dev": "concurrently \"npm run start\" \"npm run start --prefix ./example\"",
    "lint-staged": "lint-staged",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "predeploy": "cd example && npm install && npm run build",
    "prepare": "npm run build",
    "prettier": "prettier \"src/**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|mdx)\"",
    "release": "standard-version",
    "start": "rollup -c -w",
    "test:watch": "npm run test -- --watch",
    "test": "jest -c jest.config.js",
    "validate": "npm run lint && npm run prettier -- --list-different"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonathanpalma/react-simple-step-wizard.git"
  },
  "author": "Jonathan Palma <tanpalma04@gmail.com> (http://jonathanpalma.me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jonathanpalma/react-simple-step-wizard/issues"
  },
  "homepage": "https://github.com/jonathanpalma/react-simple-step-wizard#readme",
  "peerDependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "prop-types": "^15.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@svgr/rollup": "^4.3.3",
    "@testing-library/jest-dom": "^4.1.2",
    "@testing-library/react": "^9.3.0",
    "@types/jest": "^24.0.18",
    "@types/prop-types": "^15.7.3",
    "@types/react-dom": "^16.9.1",
    "@types/react": "^16.9.3",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "concurrently": "^5.0.0",
    "cz-conventional-changelog": "^3.0.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.17.1",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react-hooks": "^2.1.2",
    "eslint-plugin-react": "^7.14.3",
    "eslint": "^6.5.1",
    "gh-pages": "^2.1.1",
    "husky": "^3.0.8",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.1",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "react-dom": "^16.3.0",
    "react": "^16.3.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-typescript2": "^0.24.3",
    "rollup-plugin-url": "^2.2.2",
    "rollup": "^1.22.0",
    "standard-version": "^7.0.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3",
    "utility-types": "^3.8.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
